
<div *ngIf="sessionExpired" class="msg">
	<h3>Your Session Expired/Invalid</h3>
	please re-login <br/>
	<div style="height:2rem;"></div>
	<a [routerLink] ="['/login']" title="Login">Login</a>
</div>

<div *ngIf="!sessionExpired" class="msg">
<h3>{{message}}</h3>
<div style="height:2rem;"></div>
<a [routerLink] ="['/home']" title="Home">Home</a>
</div>