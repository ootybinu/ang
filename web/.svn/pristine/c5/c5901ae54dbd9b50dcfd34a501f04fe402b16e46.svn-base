
<app-title  title="GPRS History Data"></app-title>
<div>
	<div *ngIf="historyRows== undefined || historyRows.length == 0">
		No Data found
	</div>
	<div *ngIf="historyRows != undefined && historyRows.length > 0">
		<table class="table table-hover table-responsive col-md-12 mytable">
			<tr>
				<th>Location </th>
				<th>Division </th>
				<th>Sub Division </th>
				<th>OEM</th>
				<th>Bore Size</th>
				<th>Battery capacity</th>
				<th>Reading date </th>
				<th>Instant flow</th>
				<th>Forward</th>
				<th>Reverse flow</th>
				<th>Current </th>
				<th>Voltage</th>
				<th>unit ID</th>
				<th>Alarm</th>
			</tr>
			<tr *ngFor="let item of historyRows">
				<td> <a [routerLink]="['/gprsdata/detail', item.usId, item.readDateTime]"> {{item.location}} </a> </td>
				<td> {{item.division}}</td>
				<td> {{item.subDivision}}</td>
				<td> {{item.oEMName}}</td>
				<td> {{item.boreSize}} </td>
				<td> {{item.battery_Capacity}}</td>
				<td> {{item.readDateTime}}</td>
				<td> {{item.flowValue}}</td>
				<td> {{item.totalizer1}}</td>
				<td> {{item.totalizer2}}</td>
				<td> {{item.analoginput1value}}</td>
				<td> {{item.analoginput2value}}</td>
				<td> {{item.unitID}}</td>
				<td> {{item.alarms}}</td>
			</tr>
		</table>
	</div>
</div>