.header_text {
	font-size: 2em;
	font-weight: bold;
}
.subheader_text {
	font-size:1em;
	font-weight: bold;
}
@media (max-width: 450px) {
	.header_text {font-size: 1em;}
}
header {
	position: fixed;
	top:0;
	 /*   background: url("../assets/BG.png");*/
    background-size: cover;
	/*background: url("../assets/BG.png");*/
	border-bottom-right-radius: 10px;
	border-bottom-left-radius: 10px;
	border-bottom-color: lightgray;
    border-bottom:1px solid lightgray;
	width:100%;
	z-index: 1000;
	display: inline-flex;
	box-shadow: 0px 2px 2px 1px lightblue;
    background: -moz-linear-gradient(top, #00bdf4 0%, #224886 95%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#00bdf4), color-stop(95%,#224886));
    background: -webkit-linear-gradient(top, #00bdf4 0%,#224886 95%);
    background: -o-linear-gradient(top, #00bdf4 0%,#224886 95%);
    background: -ms-linear-gradient(top, #00bdf4 0%,#224886 95%);
    background: linear-gradient(to bottom, #00bdf4 0%,#206daa 95%);
    border-bottom:1px solid #275c9a;
	/*box-shadow: 0px 5px 5px 3px gray;*/
}
