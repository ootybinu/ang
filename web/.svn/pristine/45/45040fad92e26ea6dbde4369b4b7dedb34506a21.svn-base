
<div style="display:inline-flex;" class="col-sm-12 col-md-12 mytable1">
	<div *ngIf="searchListdata.Meter != undefined">
	<label> Meter Type </label>
	<select [(ngModel)]="selectedMeter">
		<option *ngFor="let item of searchListdata.Meter" [value]="item.key">{{item.value}}</option>
	</select>
	</div>

	<div *ngIf="searchListdata.OEM != undefined">
	<label> Manufacturer</label>
	<select [(ngModel)]="selectedOEM">
		<option *ngFor="let item of searchListdata.OEM" [value]="item.key">{{item.value}}</option>
	</select>
	</div>
</div>

<div style="display:inline-flex;" class="col-sm-12 col-md-12 mytable1">

	<div *ngIf="searchListdata.Divisions != undefined">
	<label> Division  </label>
	<select [(ngModel)]="selectedDivision" (change)="divisionChange($event)">
		<option *ngFor="let item of searchListdata.Divisions" [value]="item.key">{{item.value}}</option>
	</select>
	</div>
	<div *ngIf="subDivisions != undefined">
	<label> Sub Division  </label>
	<select [(ngModel)]="selectedSubDivision" (change)="subDivisionChange($event)">
		<option *ngFor="let item of subDivisions" [value]="item.key">{{item.value}}</option>
	</select>
	</div>

	<div *ngIf="servicesList != undefined">
	<label> Service Station  </label>
	<select [(ngModel)]="selectedService" (change)="serviceChange($event)">
		<option *ngFor="let item of servicesList" [value]="item.key">{{item.value}}</option>
	</select>
	</div>
	<div *ngIf="locationsList != undefined">
	<label> Location </label>
	<select [(ngModel)]="selectedLocation" (change)="locationChange($event)">
		<option *ngFor="let item of locationsList" [value]="item.key">{{item.value}}</option>
	</select>
	</div>

	<div *ngIf="consumerList != undefined">
	<label> Consumer </label>
	<select [(ngModel)]="selectedConsumer" (change)="locationChange($event)">
		<option *ngFor="let item of consumerList" [value]="item.key">{{item.value}}</option>
	</select>
	</div>
</div>
<div style="display:inline-flex;" class="col-sm-12 col-md-12 mytable1">
		<label> From Date </label><input type="Date" name="fromDate" [(ngModel)]="selectedFromDate" />
		<label> To Date </label><input type="Date" name="toDate" [(ngModel)]="selectedToDate"/>
	<button class="btn btn-primary" (click)="searchClicked()"> Apply Filter</button>
	<button  class="btn btn-secondary" (click)= "hidePanel()"> Cancel </button>
</div>