.printPanel
{
	height:400px;
	overflow-y:scroll;
    margin:1rem .2rem;
}
@media print {
    .myDivToPrint {
        background-color: white;
        height: 100%;
        width: 100%;
        position: fixed;
        top: 0;
        left: 0;
        margin: 0;
        padding: 15px;
        font-size: 14px;
        line-height: 18px;
    }
}
.mytable1 {border:1px solid darkgray;
border-radius:.5rem; padding:1rem;}
.mytable1 > button {padding:0px 1rem;}
.mytable1 > div {
    padding:0px 10px;
}
.mytable .btn {
width:100%;
margin :10px 20px;
}
.panel 
{
    border :1px solid gray ; 
    border-radius :.5rem;
}